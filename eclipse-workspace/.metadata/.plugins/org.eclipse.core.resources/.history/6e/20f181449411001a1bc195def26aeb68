
public class GasolineReceipt {
	
	public static void main( String[] args) {
		String d="2019-11-24 7:17AM";
		
		
		System.out.println("+---------------------------------+");
		System.out.println("|                                 |");
		System.out.println("|          CORNER STORE           |");
		System.out.println("|                                 |");
		System.out.println(" Date is: " + d );
		System.out.println("|                                 |");
		
	}
}
