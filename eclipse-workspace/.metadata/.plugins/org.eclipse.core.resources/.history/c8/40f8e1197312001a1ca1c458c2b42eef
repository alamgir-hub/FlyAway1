import java.util.Scanner;

public class ExUserInfo {
	
	public static void main( String [] args)
	{
		Scanner s = new Scanner(System.in);
		
		System.out.print("What is your age:");
		int age = s.nextInt();
		
		System.out.print("What is Salary:");
		float salary = s.nextFloat();
		
		System.out.print("your Account ID:");
		byte acc = s.nextByte();
		
		
		System.out.print("Account Balance:");
		double bal = s.nextDouble();
		
		int total = (int)(age+salary+acc+bal);		
		 //int total = age + (int)salary + (int)acc + (int)bal;		 
		 System.out.println(" combined Total :"+  total);
		 
		byte sub = (byte)(bal -salary);
		System.out.println("Subtraction of balce and salary" + sub);
		
		int mul = (int)(age*salary*acc*bal);
		System.out.println("Multiple of everthing into integer:" + mul);
		
		byte div = (byte)(bal/acc);
		System.out.println("Division of balance and account" + div);
			
				
	}
	

}
